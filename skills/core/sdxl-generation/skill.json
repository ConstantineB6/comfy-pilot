{
  "id": "sdxl-generation",
  "name": "SDXL Generation Base",
  "version": "1.0.0",
  "author": "comfy-pilot",
  "description": "Production-ready SDXL pipeline with quality settings and optimization. Perfect starting point for image generation workflows.",
  "category": "generation",
  "tags": ["sdxl", "generation", "core", "text-to-image"],
  "repository": "https://github.com/anthropics/comfy-pilot/tree/main/skills/core/sdxl-generation",

  "metadata": {
    "comfyui_min_version": "0.1.0",
    "python_min_version": "3.8",
    "dependencies": [
      "comfyui_core"
    ],
    "supported_models": [
      "sd_xl_base_1.0.safetensors",
      "sd_xl_refiner_1.0.safetensors",
      "sd_xl_turbo.safetensors"
    ],
    "stability": "production",
    "maintenance_status": "active"
  },

  "inputs": {
    "checkpoint_name": {
      "type": "string",
      "description": "Model checkpoint to load",
      "default": "sd_xl_base_1.0.safetensors",
      "required": true,
      "examples": [
        "sd_xl_base_1.0.safetensors",
        "sd_xl_turbo.safetensors"
      ]
    },
    "positive_prompt": {
      "type": "string",
      "description": "Text describing what to generate",
      "required": true,
      "examples": [
        "professional portrait, studio lighting",
        "cinematic landscape, golden hour"
      ]
    },
    "negative_prompt": {
      "type": "string",
      "description": "Text describing what to avoid",
      "default": "blurry, low quality, distorted, artifacts, bad anatomy",
      "optional": true
    },
    "width": {
      "type": "integer",
      "description": "Image width in pixels",
      "default": 1024,
      "min": 512,
      "max": 2048,
      "step": 64
    },
    "height": {
      "type": "integer",
      "description": "Image height in pixels",
      "default": 1024,
      "min": 512,
      "max": 2048,
      "step": 64
    },
    "steps": {
      "type": "integer",
      "description": "Number of sampling steps (more = higher quality but slower)",
      "default": 30,
      "min": 1,
      "max": 150,
      "notes": "20-30 for good balance, 40+ for high quality"
    },
    "cfg_scale": {
      "type": "number",
      "description": "Classifier-free guidance scale (how closely to follow prompt)",
      "default": 7.5,
      "min": 1.0,
      "max": 30.0,
      "notes": "7-8 is good default, >10 can cause distortion"
    },
    "seed": {
      "type": "integer",
      "description": "Random seed (-1 for random, any positive integer for reproducible results)",
      "default": -1,
      "min": -1
    },
    "sampler": {
      "type": "string",
      "description": "Sampling algorithm",
      "default": "dpmpp_2m",
      "options": [
        "dpmpp_2m",
        "dpmpp_3m_sde",
        "euler",
        "heun",
        "lms"
      ]
    }
  },

  "outputs": {
    "image": {
      "type": "image",
      "description": "Generated image in PNG format",
      "format": "RGBA",
      "resolution": "variable"
    },
    "metadata": {
      "type": "json",
      "description": "Generation metadata including seed, parameters, processing time",
      "schema": {
        "seed": "integer",
        "steps": "integer",
        "cfg": "number",
        "sampler": "string",
        "processing_time_ms": "integer"
      }
    }
  },

  "performance": {
    "estimated_time_seconds": 45,
    "estimated_vram_gb": 8,
    "estimated_ram_gb": 4,
    "device": "nvidia_gpu_recommended",
    "notes": "Times vary by GPU. RTX 4090: ~20s, RTX 3090: ~45s, RTX 4070: ~60s"
  },

  "nodes_created": [
    "CheckpointLoaderSimple",
    "CLIPTextEncode",
    "KSampler",
    "VAEDecode",
    "PreviewImage"
  ],

  "community_variants": [
    {
      "id": "sdxl-turbo",
      "name": "SDXL Turbo (Fast)",
      "description": "Ultra-fast generation with 4 steps",
      "url": "https://github.com/anthropics/comfy-pilot/tree/main/skills/core/sdxl-turbo"
    },
    {
      "id": "sdxl-refined",
      "name": "SDXL Refined (Quality)",
      "description": "High-quality generation with 50+ steps and refiner",
      "url": "https://github.com/anthropics/comfy-pilot/tree/main/skills/core/sdxl-refined"
    }
  ],

  "examples": [
    {
      "name": "Portrait Photography",
      "positive_prompt": "professional headshot, sharp focus, studio lighting, 50mm lens, cinematic, highly detailed",
      "negative_prompt": "blurry, oversaturated, watermark",
      "steps": 40,
      "cfg_scale": 8.0,
      "width": 768,
      "height": 1024
    },
    {
      "name": "Landscape Photography",
      "positive_prompt": "cinematic landscape, mountains, golden hour light, 8k, extremely detailed, professional photography",
      "negative_prompt": "people, buildings, blurry, artifacts",
      "steps": 35,
      "cfg_scale": 7.5,
      "width": 1280,
      "height": 768
    },
    {
      "name": "Product Shot",
      "positive_prompt": "luxury product photography, white background, professional lighting, detailed, 4k",
      "negative_prompt": "people, shadows, watermark, text",
      "steps": 30,
      "cfg_scale": 7.0,
      "width": 1024,
      "height": 1024
    }
  ],

  "rating": {
    "average": 4.8,
    "count": 1240,
    "distribution": {
      "5": 0.78,
